type Query{
    postById(id: ID): Post
    getALlPosts: [Post]
}

type Post{
    id:ID
    title: String
    description: String
    content: String
}

type Author {
    id:ID
    name: String
}

type Mutation{
    createPost(title:String!, description:String!, content:String!) : Post!
    createAuthor(name:String!) : Author!
}